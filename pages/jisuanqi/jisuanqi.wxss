.main{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.header{
  width: 100%;
  height: 200rpx;
  background: #ccc;
  margin-top:5rpx;
  margin-bottom: 20rpx;
}
.body{
  width: 100%;
}
.screen{
  width: 100%;
  line-height: 200rpx;
  text-align: right;
  padding: 0 10rpx;
  box-sizing: border-box;
  font-size: 60rpx;
  font-weight: bold;
}
.btnGroup{
  display: flex;
  flex-direction: row;
  width:100%;
  height: 200rpx;
}
.item{
  width:25%;
  display: flex;
  align-items: center;
  justify-content: center;
  line-height: 200rpx;
  margin-top: 2rpx;
  margin-right: 5rpx;
}
.fed{
  background: #f90;
}
.blue{
  background: blue;
}